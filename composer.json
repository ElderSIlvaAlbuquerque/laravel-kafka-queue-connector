{
  "name": "arquivei/laravel-kafka-queue-connector",
  "description": "Kafka queue driver for Laravel",
  "license": "Apache-2.0",
  "keywords": [
    "laravel",
    "kafka",
    "queue",
    "connector"
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/database": "^5.6",
    "illuminate/support": "^5.6",
    "illuminate/queue": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "KafkaQueueConnector\\": "src/",
      "KafkaQueueConnector\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KafkaQueueConnector\\KafkaQueueProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}